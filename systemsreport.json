    "url": "https://raw.githubusercontent.com/hannesdorn/SystemsReport/master/SystemsReport.zip",
    "bin": "systemsreport.cmd",
    "installer": {
        "script": [
            "if (!(Test-Path \"$dir\\config.ps1\")) {",
            "   Copy-Item \"$dir\\config-sample.ps1\" \"$dir\\config.ps1\"",
            "}"
        ]
    },
    "persist": [
        "config.ps1",
        "reports"
    ],
    "autoupdate": {
        "url": "https://raw.githubusercontent.com/hannesdorn/SystemsReport/master/SystemsReport.zip"
    }
}
